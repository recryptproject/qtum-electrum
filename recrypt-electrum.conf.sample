# Configuration file for the recrypt electrum client
# Settings defined here are shared across wallets
#
# copy this file to /etc/recrypt-electrum.conf if you want read-only settings

[client]
server = s1.recrypt.info:50001:t
proxy = None
gap_limit = 5
# booleans use python syntax
use_change = True
gui = qt
num_zeros = 2
# default transaction fee is in Satoshis
fee = 10000
winpos-qt = [799, 226, 877, 435]
